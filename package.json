{
  "name": "koa-rest-mongoose-seed",
  "version": "0.0.0",
  "description": "Seed for an easy and ready to go Koa REST API with Mongoose and MongoDB integration.",
  "main": "src/index.ts",
  "repository": "https://github.com/GlennyDC/koa-rest-mongoose-seed",
  "author": "Glenny De Cock",
  "license": "MIT",
  "private": false,
  "scripts": {
    "docker:dev": "docker-compose -f env/docker-compose.dev.yml up --build",
    "docker:prd": "docker-compose -f env/docker-compose.prd.yml up --build",
    "prebuild": "rimraf dist/*",
    "build": "tsc -p tsconfig.json",
    "start": "cross-env NODE_ENV=production node -r source-map-support/register dist/index.js",
    "dev": "tsc-watch --noClear --onSuccess node ./dist/index.js",
    "test": "jest --expand --verbose",
    "lint": "eslint . --cache --ext .ts,.json",
    "lint:fix": "eslint . --cache --ext .ts,.json --fix"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "cls-hooked": "^4.2.2",
    "config": "^3.3.3",
    "glob": "^7.1.6",
    "joi": "^17.3.0",
    "jsonwebtoken": "^9.0.0",
    "koa": "^2.13.0",
    "koa-bodyparser": "^4.3.0",
    "koa-helmet": "^6.0.0",
    "koa-router": "^10.0.0",
    "mongoose": "^5.10.15",
    "nanoid": "^3.1.20",
    "node-fetch": "^2.6.1",
    "source-map-support": "^0.5.19",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/cls-hooked": "^4.3.1",
    "@types/config": "^0.0.37",
    "@types/glob": "^7.1.3",
    "@types/jest": "^26.0.16",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-helmet": "^6.0.1",
    "@types/koa-router": "^7.4.1",
    "@types/mongoose": "^5.10.0",
    "@types/node": "^14.14.10",
    "@types/node-fetch": "^2.5.7",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "cross-env": "^7.0.3",
    "eslint": "6.8.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^30.7.8",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "husky": ">=4",
    "jest": "^26.6.3",
    "lint-staged": ">=10",
    "prettier": "2.2.1",
    "rimraf": "^3.0.2",
    "supertest": "^6.0.1",
    "ts-jest": "^26.4.4",
    "tsc-watch": "^4.2.9",
    "typescript": "^4.0.5"
  }
}
